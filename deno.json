  },
  "imports": {
    "react/jsx-runtime": "https://esm.sh/react@18.2.0/jsx-runtime",
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@513220bcdab0172e7b0a0c2dd66b928ce907190c/"
  }
}
